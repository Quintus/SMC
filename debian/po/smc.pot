msgstr ""

#. type: Plain text
#: debian/tsc.6:35
msgid "Show version of program."
msgstr ""

#. type: TP
#: debian/tsc.6:35
#, no-wrap
msgid "B<-d, --debug>"
msgstr ""

#. type: Plain text
#: debian/tsc.6:38
msgid "Enable debug modes with the options : game performance."
msgstr ""

#. type: TP
#: debian/tsc.6:38
#, no-wrap
msgid "B<-l, --level>"
msgstr ""

#. type: Plain text
#: debian/tsc.6:41
